<?xml version="1.0" encoding="UTF-8"?>

<suite name="Acceptance-EmailServers" parallel="false">
    <listeners>
      <listener class-name="org.uncommons.reportng.HTMLReporter"/>
      <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
      <listener class-name="org.roommanager.utils.TestNGCustom"/>
  	</listeners>
   	<test name="RegisterEmailServer">
    	<classes>
	      	<class name="org.roommanager.test.admin.emailserver.RegisterEmailServer" />
    	</classes>
  	</test>
  	<test name="RemoveEmailServer">
    	<classes>
    		<class name="org.roommanager.test.admin.emailserver.RemoveEmailServer" />
    	</classes>
  	</test>
  	<test name="EnableImpersonation">
    	<classes>
	      	<class name="org.roommanager.test.admin.impersonation.EnableImpersonation" />
    	</classes>
  	</test>
</suite>